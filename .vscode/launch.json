{
	"version": "0.2.0",
	"configurations": [
		{
			"name": ".NET Core Launch (console)",
			"type": "coreclr",
			"request": "launch",
			"preLaunchTask": "build",
			"program": "${workspaceRoot}/bin/Debug/net4.6.2/OddSpell.exe",
			"args": ["add", "arete"],
			"cwd": "${workspaceRoot}",
			"stopAtEntry": false,
			"externalConsole": false
		},
		{
			"name": ".NET Core Attach",
			"type": "coreclr",
			"request": "attach",
			"processId": "${command.pickProcess}"
		}
	]
}